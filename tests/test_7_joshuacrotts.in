void isort(int[] arr) {
    int i;
    int key;
    int j;  
    
    for (i = 1; i < #arr; i++) 
    {  
        key = arr[i];  
        j = i - 1;  
  
        while (j >= 0 && arr[j] > key) 
        {  
            arr[j + 1] = arr[j];  
            j = j - 1;  
        }  
        arr[j + 1] = key;  
    }  
}

void swap(int[] arr, int i, int j) {
	int tmp;
	tmp = arr[i];
	arr[i] = arr[j];
	arr[j] = tmp;
}

void reversearr(int[] arr) {
	int i;
	for(i=0;i<#arr/2;i++)
		swap(arr, i, #arr-i-1);
} 

void mod(int[] arr) {
	int i;
	for(i=0;i<#arr;i++)
		arr[i] = 5;
}

void parray(int[] arr) {
